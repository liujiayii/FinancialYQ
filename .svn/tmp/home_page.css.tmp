.container{
	position: relative;
}
.head_box{
	height: 123px;
	width: 100%;
	position: fixed;
	top: 0;
	z-index: 999;
	min-width: 1000px;
}
.head{
	width: 100%;
	height: 60px;
	box-sizing: border-box;
	padding:0 35px 0 55px;
	background: url(../images/bg.png) no-repeat;
	border-bottom: 1px solid #0CA1EF;
	z-index: 999;
}
.head_left{
	float: left;
}
.head_right{
	float: right;
}
.head_right b{
	width: 100px;
	display: inline-block;
	font-size: 18px;
	color: #59CAFF;
	line-height: 60px;
}
.head_right img{
	margin-top: 15px;
    float: right;
}
.classify_box{
	width: 100%;
	height: 63px;
	border-bottom: 1px solid #eeeeee;
	box-sizing: border-box;
	padding:0 35px 0 55px;
	background: #FFFFFF;
}
.classify_uls{
	width: 80%;
	height: 100%;
	line-height: 65px;
	float: left;
	/* overflow: hidden; */
}
.classify_uls>li{
	float: left;
	margin-right: 30px;
	font-size: 18px;
	text-align: center;
}
.classify_uls a:hover,.classify_uls .selected{
	color: #8907b9;
	font-weight: 600;
}
.content{
	width: 1800px;
	margin: 123px auto 0;
	padding-bottom: 100px;
}
.sub-box{
	line-height: 35px;
	width: 100%;
	overflow: hidden;
	padding: 5px 0px 10px 0;
	box-sizing: border-box;
}
.sub-right{
	float: right;
}
.sub-search{
	width: 250px;
	height: 35px;
	border: 1px solid #dddddd;
	border-radius: 3px;
	font-size: 14px;
	padding-left: 10px;
	margin-top:4px;
	margin-right: 15px;
	background: url(../images/seek.png) no-repeat center;
	background-position-x: 215px;
}
.sub-button{
	width: 100px;
	height: 40px;
	color: #fff;
	border: none;
	font-size: 14px;
	border-radius: 7px;
	background: #568bfc;
	float: right;
	margin-right: 8px;
	cursor: pointer;
	box-shadow: 1px 3px 14px #568bfce6;
	text-align: center;
	line-height: 40px;
}
.sub-but{
	width: 100px;
	height: 40px;
	color: #fff;
	border: none;
	font-size: 14px;
	border-radius: 7px;
	background: #f55b63;
	float: right;
	margin-right: 8px;
	cursor: pointer;
	box-shadow: 1px 3px 14px rgb(252,194,196);
	text-align: center;
	line-height: 40px;
}
.sub-nav li{
	float: left;
	margin-right: 20px;
	font-size: 16px;
}
