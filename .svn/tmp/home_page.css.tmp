.sub-nav li a{
	display: inline-block;
	cursor:pointer;
	border-bottom: 2px solid #fff;
}
.sub-nav a:hover,.selected{
	font-weight: 550;
	border-bottom: 2px solid #0CA1EF !important;
}
#sub-add{
	margin: 13px;
	cursor: pointer;
}
/*表格*/
	a:focus {outline: none;}
	.table_tbs{position:relative; width: 100%;border-color: #EEEEEE;margin-top: 3px;}
	.table_tbs thead{background: #b3a0f2;color: #fff;}
	.table_tbs thead tr{height: 60px !important;}
	.table_tbs tbody{font-size:14px;}
	.table_tbs tbody tr{height: 75px !important;text-align: center}
	.table_tbs tbody tr:hover{background-color:#1f6ee812 !important;}
	.table_tbs tfoot tr {height: 160px;text-align: center;border-bottom: 1px solid #cfcfcf;}
/*表格分页列表*/
	.table_tbs td.paging a {text-decoration: none !important;border: 1px solid #CFCFCF; color: #969696; display: inline-block;height: 20px;border-radius: 5px;padding: 7px;margin-left: 10px;}
	.table_tbs td.paging a.current { color: #444; font-weight: bold;display: inline-block;height: 20px;border-radius: 5px;padding: 7px;}
/*表格分页当前页*/

