.pp{
	height: 55px;
	line-height: 55px;
	text-align: center;
	color: #232323;
	font-size: 14px;
	letter-spacing: 2px;
	border-top: 1px solid #eeeeee;
}
/*验证*/
.explain1,.explain2,.explain3{
	height: 20px;
    border: none;
    line-height: 20px;
    margin-bottom: 5px;
    color:red;
    font-size: 13px;
}
.explain3{margin-top: 5px;}
